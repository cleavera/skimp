{
  "name": "@skimp/test",
  "version": "1.0.0",
  "description": "Testing module for skimp",
  "scripts": {
    "build": "tsc -p \"./tsconfig.json\"",
    "clean": "del-cli \"./.test/**/*.*\" && del-cli \"./.artifacts/**/*.*\"",
    "lint": "npx eslint ./src/**/*.ts --config=../../.eslintrc.json",
    "postinstall": "npm run sync",
    "sync": "subrosa update --prefix=\"skimp\" --root=\"../../\"",
    "test": "npm run clean && npm run sync && npm run test-run",
    "test-run": "npm run build && alsatian ./.test/**/*.spec.js --tap"
  },
  "author": "Anthony Cleaver",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/request": "2.48.7",
    "@types/request-promise-native": "1.0.18",
    "@types/uuid": "8.3.3",
    "alsatian": "^3.2.1",
    "del-cli": "^4.0.1",
    "glob": "^7.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "typescript": "^4.5.3",
    "uuid": "8.3.2"
  },
  "peerDependencies": {
    "@skimp/core": "*",
    "@skimp/debug": "*",
    "@skimp/file-system": "*",
    "@skimp/http": "*",
    "@skimp/json-api": "*",
    "@skimp/json-file": "*",
    "@skimp/quickstart": "*",
    "@skimp/router": "*",
    "@skimp/schema": "*",
    "@skimp/server": "*",
    "@skimp/validation": "*"
  },
  "dependencies": {
    "@cleavera/utils": "^4.2.1"
  }
}
