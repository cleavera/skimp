{
  "name": "@skimp/quickstart",
  "version": "2.1.0",
  "description": "The quickstart module for the skimp domain modelling framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -X -f -d --exclude=\"!node_modules\"",
    "example": "ts-node ./example/run",
    "lint": "exit 0 && tslint --project ./tsconfig.json",
    "test": "echo \"Tests are run in the testing module\" && exit 1",
    "prepack": "npm run clean && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleavera/skimp.git"
  },
  "keywords": [
    "skimp",
    "quickstart",
    "domain",
    "modelling",
    "framework"
  ],
  "author": "Anthony Cleaver",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@cleavera/utils": "^3.6.1",
    "@skimp/core": "^3.0.0",
    "@skimp/debug": "^1.3.0",
    "@skimp/file-system": "^2.1.0",
    "@skimp/http": "^4.0.0",
    "@skimp/json-api": "^3.0.1",
    "@skimp/json-file": "^3.2.0",
    "@skimp/router": "^2.0.0",
    "@skimp/schema": "^2.5.0",
    "@skimp/server": "^4.0.0",
    "@skimp/validation": "^2.1.0"
  }
}
