{
  "name": "@skimp/test",
  "version": "1.0.0",
  "description": "Testing module for skimp",
  "scripts": {
    "build": "tsc -p \"./tsconfig.json\"",
    "clean": "del-cli \"./.test/**/*.*\" && del-cli \"./.artifacts/**/*.*\"",
    "lint": "tslint --project ./tsconfig.json",
    "postinstall": "npm run sync",
    "sync": "node ./scripts/install",
    "test": "npm run clean && npm run sync && npm run test-run",
    "test-run": "npm run build && alsatian ./.test/**/*.spec.js --tap"
  },
  "author": "Anthony Cleaver",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/node": "^13.7.4",
    "@types/request": "2.47.1",
    "@types/request-promise-native": "1.0.15",
    "@types/uuid": "3.4.3",
    "alsatian": "^3.0.0",
    "del-cli": "^2.0.0",
    "glob": "^7.1.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "tslint": "^5.18.0",
    "typescript": "^3.8.2",
    "uuid": "3.3.2"
  },
  "peerDependencies": {
    "@skimp/core": "*",
    "@skimp/debug": "*",
    "@skimp/file-system": "*",
    "@skimp/http": "*",
    "@skimp/json-api": "*",
    "@skimp/json-file": "*",
    "@skimp/quickstart": "*",
    "@skimp/router": "*",
    "@skimp/schema": "*",
    "@skimp/server": "*",
    "@skimp/validation": "*"
  },
  "dependencies": {
    "@cleavera/utils": "^3.6.1"
  }
}
